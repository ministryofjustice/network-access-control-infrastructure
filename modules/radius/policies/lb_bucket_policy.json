{
  "Version": "2012-10-17",
  "Id": "ConfigFetch",
  "Statement": [
      {
          "Sid": "Allow Load Balancer to set logs",
          "Effect": "Allow",
          "Principal": {
            "AWS": "${load_balancer_arn}"
          },
          "Action": [
              "*"
          ],
          "Resource": "${bucket_arn}/*"
      }
  ]
}
