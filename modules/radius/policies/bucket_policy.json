{
    "Version": "2012-10-17",
    "Id": "ConfigFetch",
    "Statement": [
        {
            "Sid": "Get configuration file",
            "Effect": "Allow",
            "Principal": {
              "AWS": "${ecs_task_role_arn}"
            },
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListObjectsV2"
            ],
            "Resource": "${bucket_arn}/*"
        }
    ]
}
